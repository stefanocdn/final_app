<%= f.label :company_name %>
<%= f.text_field :company_name, data: {autocomplete_source: schools_path} %><br/>

<%= f.label :city %>
<%= f.text_field :city %><br/>

<%= f.label :country %>
<%= f.text_field :country %><br/>


<%= f.label :start_date %>
<%= f.date_select :start_date, discard_month: true, start_year: 1995 %><br />

<%= f.label :end_date %>
<%= f.date_select :end_date, discard_month: true, start_year: 1995  %><br />

<%= f.submit "Save Position", class: "btn btn-xs btn-primary" %>